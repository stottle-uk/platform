{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "web": {
      "root": "apps/web/",
      "sourceRoot": "apps/web/src",
      "projectType": "application",
      "prefix": "stottle-platform",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web",
            "index": "apps/web/src/index.html",
            "main": "apps/web/src/main.ts",
            "polyfills": "apps/web/src/polyfills.ts",
            "tsConfig": "apps/web/tsconfig.app.json",
            "assets": ["apps/web/src/favicon.ico", "apps/web/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/web/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web/src/environments/environment.ts",
                  "with": "apps/web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/web/src/test.ts",
            "polyfills": "apps/web/src/polyfills.ts",
            "tsConfig": "apps/web/tsconfig.spec.json",
            "karmaConfig": "apps/web/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/web/src/styles.css"
            ],
            "scripts": [],
            "assets": ["apps/web/src/favicon.ico", "apps/web/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web/tsconfig.app.json",
              "apps/web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "web-e2e": {
      "root": "apps/web-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/web-e2e/protractor.conf.js",
            "devServerTarget": "web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/web-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ngrx-auth0": {
      "root": "libs/ngrx-auth0",
      "sourceRoot": "libs/ngrx-auth0/src",
      "projectType": "library",
      "prefix": "stottle-platform",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/ngrx-auth0/tsconfig.lib.json",
            "project": "libs/ngrx-auth0/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ngrx-auth0/src/test.ts",
            "tsConfig": "libs/ngrx-auth0/tsconfig.spec.json",
            "karmaConfig": "libs/ngrx-auth0/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ngrx-auth0/tsconfig.lib.json",
              "libs/ngrx-auth0/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "auth0-rxjs": {
      "root": "libs/auth0-rxjs",
      "sourceRoot": "libs/auth0-rxjs/src",
      "projectType": "library",
      "prefix": "stottle-platform",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/auth0-rxjs/tsconfig.lib.json",
            "project": "libs/auth0-rxjs/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/auth0-rxjs/src/test.ts",
            "tsConfig": "libs/auth0-rxjs/tsconfig.spec.json",
            "karmaConfig": "libs/auth0-rxjs/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/auth0-rxjs/tsconfig.lib.json",
              "libs/auth0-rxjs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/schematics:module": {
      "flat": false,
      "spec": false,
      "project": "web"
    },
    "@nrwl/schematics:component": {
      "prefix": "stottle",
      "styleext": "scss",
      "inlineStyle": true,
      "inlineTemplate": true,
      "flat": true,
      "spec": true,
      "project": "web"
    },
    "@nrwl/schematics:service": {
      "flat": true,
      "project": "web"
    },
    "@nrwl/schematics:directive": {
      "prefix": "stottle",
      "project": "web"
    },
    "@nrwl/schematics:pipe": {
      "project": "web"
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "defaultProject": "web"
}
